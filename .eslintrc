{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "jest"
  ],
  "extends": [
    "plugin:jest/recommended"
  ],
  "rules": {
    "semi": 2
  },
  "env": {
    "es6": true,
    "jest/globals": true
  }
}